<section class="faq">
  <h2 class="faq__title">{{ faq.title }}</h2>
  <ul class="faq__list">
    {% for item in faq.items %}
      <li class="faq__item">
        <h3 class="faq__question">{{ item.question }}</h3>
        <p class="faq__answer">{{ item.answer }}</p>
      </li>
    {% endfor %}
  </ul>
</section>
